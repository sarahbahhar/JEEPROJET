<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="Model.Infocompte" table="infocompte" schema="projet_jee">
        <property name="email" column="email"/>
        <property name="prenom" column="prenom"/>
        <property name="nom" column="nom"/>
        <property name="dateNaissance" column="dateNaissance"/>
        <property name="telephone" column="telephone"/>
        <property name="adresse" column="adresse"/>
        <property name="ville" column="ville"/>
        <property name="codePostal" column="codePostal"/>
        <property name="pays" column="pays"/>
    </class>
</hibernate-mapping>