<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="Model.Produitcommande" table="produitcommande" schema="projet_jee">
        <composite-id mapped="true" class="Model.ProduitcommandePK">
            <key-property name="produitId" column="produit_id"/>
            <key-property name="commandeNumero" column="commande_numero"/>
        </composite-id>
        <property name="quantite" column="quantite"/>
    </class>
</hibernate-mapping>