<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="https://jakarta.ee/xml/ns/persistence/orm"
                 version="3.0">

    <entity class="Model.Produitpanier">
        <table name="produitpanier" schema="projet_jee" catalog=""/>
        <id-class class="Model.ProduitpanierPK"/>
        <attributes>
            <id name="email">
                <column name="email" length="100"/>
            </id>
            <id name="produitId">
                <column name="produit_id" precision="-1"/>
            </id>
            <basic name="quantite">
                <column name="quantite" precision="-1"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
