<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="https://jakarta.ee/xml/ns/persistence/orm"
                 version="3.0">

    <entity class="Model.Comptebancaire">
        <table name="comptebancaire" schema="projet_jee" catalog=""/>
        <attributes>
            <id name="id">
                <column name="id" precision="-1"/>
            </id>
            <basic name="nom">
                <column name="nom"/>
            </basic>
            <basic name="numero">
                <column name="numero"/>
            </basic>
            <basic name="date">
                <column name="date" precision="-1"/>
            </basic>
            <basic name="cvv">
                <column name="cvv" length="3"/>
            </basic>
            <basic name="email">
                <column name="email" length="100"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
