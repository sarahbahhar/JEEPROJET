<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="https://jakarta.ee/xml/ns/persistence/orm"
                 version="3.0">

    <entity class="Model.Commande">
        <table name="commande" schema="projet_jee" catalog=""/>
        <attributes>
            <id name="numero">
                <column name="numero" precision="-1"/>
            </id>
            <basic name="dateDePaiement">
                <column name="dateDePaiement" precision="-1"/>
            </basic>
            <basic name="total">
                <column name="total" precision="2"/>
            </basic>
            <basic name="email">
                <column name="email" length="100"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
