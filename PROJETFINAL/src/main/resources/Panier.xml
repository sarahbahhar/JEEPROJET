<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="https://jakarta.ee/xml/ns/persistence/orm"
                 version="3.0">

    <entity class="Model.Panier">
        <table name="panier" schema="projet_jee" catalog=""/>
        <attributes>
            <id name="email">
                <column name="email" length="100"/>
            </id>
            <basic name="ht">
                <column name="HT" precision="2"/>
            </basic>
            <basic name="tva">
                <column name="TVA" precision="2"/>
            </basic>
            <basic name="ttc">
                <column name="TTC" precision="2"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
