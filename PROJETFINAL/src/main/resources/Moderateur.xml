<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="https://jakarta.ee/xml/ns/persistence/orm"
                 version="3.0">

    <entity class="Model.Moderateur">
        <table name="moderateur" schema="projet_jee" catalog=""/>
        <attributes>
            <id name="email">
                <column name="email" length="100"/>
            </id>
            <basic name="peutAjouterProduit">
                <column name="peut_ajouter_produit" precision="1"/>
            </basic>
            <basic name="peutSupprimerProduit">
                <column name="peut_supprimer_produit" precision="1"/>
            </basic>
            <basic name="maxProduitsLigne">
                <column name="max_produits_ligne" precision="-1"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
