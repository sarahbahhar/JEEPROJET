<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="Model.Comptebancaire" table="comptebancaire" schema="projet_jee">
        <id name="identifiant">
            <column name="identifiant" sql-type="int"/>
        </id>
        <property name="nom">
            <column name="nom" sql-type="varchar(255)" not-null="true"/>
        </property>
        <property name="numero">
            <column name="numero" sql-type="varchar(255)" not-null="true"/>
        </property>
        <property name="date">
            <column name="date" sql-type="date" not-null="true"/>
        </property>
        <property name="cvv">
            <column name="cvv" sql-type="varchar(3)" length="3" not-null="true"/>
        </property>
        <property name="email">
            <column name="email" sql-type="varchar(100)" length="100" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>