<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="https://jakarta.ee/xml/ns/persistence/orm"
                 version="3.0">

    <entity class="Model.Produitcommande">
        <table name="produitcommande" schema="projet_jee" catalog=""/>
        <id-class class="Model.ProduitcommandePK"/>
        <attributes>
            <id name="produitId">
                <column name="produit_id" precision="-1"/>
            </id>
            <id name="commandeNumero">
                <column name="commande_numero" precision="-1"/>
            </id>
            <basic name="quantite">
                <column name="quantite" precision="-1"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
