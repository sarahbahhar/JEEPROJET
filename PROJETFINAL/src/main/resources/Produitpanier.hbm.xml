<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="Model.Produitpanier" table="produitpanier" schema="projet_jee">
        <property name="produitId">
            <column name="produit_id" sql-type="int"/>
        </property>
        <property name="quantite">
            <column name="quantite" sql-type="int"/>
        </property>
    </class>
</hibernate-mapping>