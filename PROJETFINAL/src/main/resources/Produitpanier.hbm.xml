<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="Model.Produitpanier" table="produitpanier" schema="projet_jee">
        <composite-id mapped="true" class="Model.ProduitpanierPK">
            <key-property name="email">
                <column name="email" sql-type="varchar(100)" length="100"/>
            </key-property>
            <key-property name="produitId">
                <column name="produit_id" sql-type="int"/>
            </key-property>
        </composite-id>
        <property name="quantite">
            <column name="quantite" sql-type="int"/>
        </property>
    </class>
</hibernate-mapping>