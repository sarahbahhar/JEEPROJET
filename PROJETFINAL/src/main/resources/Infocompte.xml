<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="https://jakarta.ee/xml/ns/persistence/orm"
                 version="3.0">

    <entity class="Model.Infocompte">
        <table name="infocompte" schema="projet_jee" catalog=""/>
        <attributes>
            <id name="email">
                <column name="email" length="100"/>
            </id>
            <basic name="prenom">
                <column name="prenom" length="50"/>
            </basic>
            <basic name="nom">
                <column name="nom" length="50"/>
            </basic>
            <basic name="dateNaissance">
                <column name="dateNaissance" precision="-1"/>
            </basic>
            <basic name="telephone">
                <column name="telephone" length="10"/>
            </basic>
            <basic name="adresse">
                <column name="adresse" length="250"/>
            </basic>
            <basic name="ville">
                <column name="ville" length="50"/>
            </basic>
            <basic name="codePostal">
                <column name="codePostal" precision="-1"/>
            </basic>
            <basic name="pays">
                <column name="pays" length="50"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
