<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="Model.Produitcommande" table="produitcommande" schema="projet_jee">
        <composite-id mapped="true" class="Model.ProduitcommandePK">
            <key-property name="produitId">
                <column name="produit_id" sql-type="int"/>
            </key-property>
            <key-property name="commandeNumero">
                <column name="commande_numero" sql-type="int"/>
            </key-property>
        </composite-id>
        <property name="quantite">
            <column name="quantite" sql-type="int" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>