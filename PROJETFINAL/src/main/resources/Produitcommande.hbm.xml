<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="Model.Produitcommande" table="produitcommande" schema="projet_jee">
        <id name="id">
            <column name="id" sql-type="int"/>
        </id>
        <property name="commandeNumero">
            <column name="commande_numero" sql-type="int"/>
        </property>
        <property name="quantite">
            <column name="quantite" sql-type="int"/>
        </property>
        <property name="titre">
            <column name="titre" sql-type="varchar(255)"/>
        </property>
        <property name="prix">
            <column name="prix" sql-type="decimal(10,2)" precision="10" scale="2"/>
        </property>
    </class>
</hibernate-mapping>